import discord
from discord.ext import commands


class CogName(commands.Cog):
	"""
 	Создание класса кога бота, внутри него нужно создавать команды в виде функций.
	"""

	def __init__(self, bot):
		self.bot = bot


def setup(bot):
	"""
	Функция для добавления кога в бота.	
	"""

	bot.add_cog(CogName(bot))
	print(f"Ког {CogName} к работе готов!")
]]></content>
	<tabTrigger>discord.py cog</tabTrigger>
</snippet>
