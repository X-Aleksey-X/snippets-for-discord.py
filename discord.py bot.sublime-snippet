import discord
from discord.ext import commands


bot = commands.Bot(command_prefix='!')
bot.run("Токен бота.")


@bot.event
async def on_ready():
	print(f"Бот {bot.user} вошёл в сеть, ID: {bot.user.id}") 
]]></content>
